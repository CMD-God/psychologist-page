
.btn-apricot {
	color: var(--darktext);
	background-color: var(--apricot);
	border-color: var(--apricot);
}

.contact_bar {
	background-color: var(--sanddune);
	font-size: 0.8em;
	padding: 0.2em 15%;
	text-align: right;
	position: fixed;
	top: 0px;
	left: 0px;
	height: 2em;
	width: 100%;
}

.navbar_space {
	height: 5em;
}

.navbar {
	top:2em;
	left: 0px;
	width: 100%;
	height: 3em;
	background-color: #FFFFFF;
	align-content: space-around;
	justify-content: center;
}

.navbar_name {
	color: var(--sanddune);
	margin-right: 3em;
}

.navbar_link {
	margin: 0.8em;
}

a {
	text-decoration: none!important;
}

.navbar_link a {
	color: var(--dovegray);
}

.navbar_link a:hover {
	color: var(--apricot);
}

.navbar_link a:active {
	color: var(--tacao);
}

.white_text {
	color: white;
}

